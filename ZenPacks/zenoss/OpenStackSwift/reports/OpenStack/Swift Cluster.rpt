<tal:block tal:define="
    clusters python:here.ReportServer.plugin('swift_clusters', {});">

<tal:block metal:use-macro="here/reportMacros/macros/exportableReport">
<tal:block metal:fill-slot="report">

<tal:block metal:use-macro="here/templates/macros/page1">
<tal:block metal:fill-slot="contentPane">

<table>
    <thead>
    <tr>
        <th></th>
        <th>Status</th>
        <th>Performance</th>
        <th>Capacity</th>
    </tr>
    </thead>

    <tbody>
    <tr tal:repeat="cluster clusters">
        <th tal:content="cluster/title"/>
        <td><table tal:replace="structure cluster/status"/></td>
        <td><table tal:replace="structure cluster/performance"/></td>
        <td><table tal:replace="structure cluster/capacity"/></td>
    </tr>
    </tbody>
</table>

</tal:block>
</tal:block>

</tal:block>
</tal:block>

</tal:block>
